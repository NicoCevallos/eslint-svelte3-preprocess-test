{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "noEmitHelpers": true,
        "noEmitOnError": true,
        "skipLibCheck": true,
        "lib": [
            "es6"
        ],
        "types": [
            "./node_modules/svelte/"
        ],
        "baseUrl": ".",
    },
    "exclude": [
        "node_modules",
    ]
}
